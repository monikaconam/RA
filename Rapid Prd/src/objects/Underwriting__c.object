    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <startsWith>Vowel</startsWith>
    <validationRules>
        <fullName>All_Approved_With_Conditions_Met</fullName>
        <active>true</active>
        <description>Can&apos;t check it if &quot;Approved With conditions&quot; field is not blank</description>
        <errorConditionFormula>AND(NOT( ISBLANK( Approved_With_Conditions__c ) ), All_Approved_With_Conditions_Met__c )</errorConditionFormula>
        <errorDisplayField>All_Approved_With_Conditions_Met__c</errorDisplayField>
        <errorMessage>Need to satisfy all conditions first</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Business_Tax_Lien</fullName>
        <active>true</active>
        <description>The business tax lien information must be completed before rendering an decision.</description>
        <errorConditionFormula>contains( TEXT( Underwriting_Decision__c ),&quot;Approved&quot; ) &amp;&amp; isblank(Business_Open_Tax_Liens__c)</errorConditionFormula>
        <errorMessage>Please fill out the Business Tax Lien questions on the corresponding Opportunity Page before rendering the Decision.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Business_Tax_Lien_25k_or_more</fullName>
        <active>true</active>
        <description>Additional business tax lien information must be completed before rendering an decision.</description>
        <errorConditionFormula>contains( TEXT( Underwriting_Decision__c ),&quot;Approved&quot; ) &amp;&amp; 
or(Business_Open_Tax_Liens__c=&quot;$25k-$50k&quot;,Business_Open_Tax_Liens__c=&quot;$50k-$75k&quot;,Business_Open_Tax_Liens__c=&quot;$75k and Above&quot;)&amp;&amp;
or(isblank( Business_Open_Tax_Lien_Type__c ),isblank( Business_Tax_Lien_Date_Filed__c ))</errorConditionFormula>
        <errorMessage>Please fill out the Additional Business Tax Lien questions on the corresponding Opportunity Page before rendering the Decision.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Initial_File_Review</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(ISBLANK(TEXT( Underwriting_Decision__c ))) &amp;&amp; Initial_File_Review_Complete__c=FALSE &amp;&amp;
DATEVALUE(CreatedDate) &gt;= DATE(2013,1,29)</errorConditionFormula>
        <errorDisplayField>Initial_File_Review_Complete__c</errorDisplayField>
        <errorMessage>Please confirm that the Initial File Review has been completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_FRSA_Needed_New_Pricing</fullName>
        <active>true</active>
        <description>If &quot;New FRSA Needed&quot; field is populated, all new pricing fields must be populated.</description>
        <errorConditionFormula>OR(
AND(Deal_Repriced_Need_New_FRSA__c  = NULL, OR( New_Advance_Price__c &lt;&gt;NULL, New_Percent_Payback__c&lt;&gt;NULL, New_Payback_Price__c &lt;&gt;NULL) ),
AND(Deal_Repriced_Need_New_FRSA__c&lt;&gt;NULL, OR( New_Advance_Price__c =NULL, New_Percent_Payback__c =NULL, New_Payback_Price__c =NULL) )
)</errorConditionFormula>
        <errorDisplayField>X10_Day_Average_Daily_Repay__c</errorDisplayField>
        <errorMessage>If a new FRSA is needed, you must indicate the New Advance Price, New Percent Payback, and New Payback Price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pre_Approval_Check</fullName>
        <active>true</active>
        <errorConditionFormula>contains( TEXT( Underwriting_Decision__c ),&quot;Approved&quot; ) &amp;&amp; OR((contains( Product__c ,&quot;loan&quot;) &amp;&amp; OR((Opportunity__r.Loan_Term_Days__c  &lt;= 0),isblank(Opportunity__r.Loan_Term_Days__c))), ISBLANK( Other_UCC_Filings__c ),  ISBLANK( text(Business_Name_On_Check_Reference__c )) ,   ISBLANK( text(Address_on_Check_Match_Bank_Statement__c) ) , ISBLANK( text(Address_on_Check_Match_CC_statements__c) ) ,isblank( Does_Merchant_Own_The_Business_Location__c ), ISBLANK( text(Address_on_Check_Match_FRSA__c )), ISBLANK( Business_Open_Date__c ) , ISBLANK( Owned_Business_Since__c ) , ISBLANK( text(Business_Bankruptcies__c )) ,  ISBLANK( Business_Status__c ) , ISBLANK( Type_of_Entity__c ), ISBLANK( text( Called_Confirmed_Add_l_Signer_Email__c ) ) ,ISBLANK( text( Called_Confirmed_Add_l_Signer_Number__c ) ) ,ISBLANK( text( Called_Confirmed_Contact_Email__c ) ) ,ISBLANK( text( Called_Confirmed_Contact_Number__c ) ) ,CONTAINS(Type_of_Entity__c, &quot;Confirm&quot;)   )</errorConditionFormula>
        <errorMessage>Missing Info, Please Review</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rapid_Funding_Check</fullName>
        <active>true</active>
        <description>UW Must Determine Rapid Funding Eligibility before Approving.</description>
        <errorConditionFormula>and(ispickval(Eligible_for_Rapid_Funding__c,&quot;&quot;), left(text(Underwriting_Decision__c),4)=&quot;Appr&quot;)</errorConditionFormula>
        <errorDisplayField>Eligible_for_Rapid_Funding__c</errorDisplayField>
        <errorMessage>Determine Rapid Funding Eligibility before Approving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UCC_Date_Validation</fullName>
        <active>true</active>
        <description>Need to choose a date when UCC is selected</description>
        <errorConditionFormula>AND( OR(ISPICKVAL( UCC_Filings__c ,&quot;Full UCC-1&quot;),ISPICKVAL(UCC_Filings__c,&quot;Notification UCC&quot;)), ISNULL( UCC_Request_Date__c  ))</errorConditionFormula>
        <errorMessage>Please Select a UCC Date.</errorMessage>
    </validationRules>
</CustomObject>
